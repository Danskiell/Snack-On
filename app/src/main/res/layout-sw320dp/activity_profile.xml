<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".ProfileActivity">


    <View
        android:id="@+id/rectangle"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/profile_rectangle"
        android:elevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ConfigLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:text="Configurações"
        android:textColor="@color/white"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageBack"
        app:layout_constraintStart_toEndOf="@+id/imageBack"
        app:layout_constraintTop_toTopOf="@+id/imageBack"></TextView>

    <ImageView
        android:id="@+id/imageBack"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:elevation="6dp"
        android:onClick="fechar"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/arrow_circle"></ImageView>

    <View
        android:id="@+id/PhotoFrame"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/back_circle"
        android:elevation="2dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle"></View>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:elevation="4dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/PhotoFrame"
        app:layout_constraintEnd_toEndOf="@+id/PhotoFrame"
        app:layout_constraintHorizontal_bias="0.575"
        app:layout_constraintStart_toStartOf="@+id/PhotoFrame"
        app:layout_constraintTop_toTopOf="@+id/PhotoFrame"
        app:layout_constraintVertical_bias="0.45"
        app:srcCompat="@drawable/icon_cam" />

    <TextView
        android:id="@+id/daniel_alve"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="60dp"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:text="Nome usuário"
        android:textAppearance="@style/daniel_alve"
        app:layout_constraintStart_toEndOf="@+id/PhotoFrame"
        app:layout_constraintTop_toBottomOf="@+id/rectangle" />

    <TextView
        android:id="@+id/cliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:text="Email usuário"
        android:textAppearance="@style/cliente"
        app:layout_constraintBottom_toBottomOf="@+id/PhotoFrame"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toEndOf="@+id/PhotoFrame"
        app:layout_constraintTop_toBottomOf="@+id/daniel_alve"
        app:layout_constraintVertical_bias="0.107" />

    <TextView
        android:id="@+id/phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:text="Telefone usuário"
        android:textAppearance="@style/cliente"
        app:layout_constraintBottom_toBottomOf="@+id/PhotoFrame"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toEndOf="@+id/PhotoFrame"
        app:layout_constraintTop_toBottomOf="@+id/daniel_alve"
        app:layout_constraintVertical_bias="0.476" />


    <View
        android:id="@+id/view"
        android:layout_width="350dp"
        android:layout_height="66dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="90dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/navigation_container"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"></View>

    <ImageView
        android:id="@+id/iconHome"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginEnd="292dp"
        android:elevation="4dp"
        android:onClick="gohome"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.486"
        app:srcCompat="@drawable/icon_home" />

    <ImageView
        android:id="@+id/iconCart"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:elevation="4dp"
        android:onClick="carrinho"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.402"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.476"
        app:srcCompat="@drawable/icon_cart" />

    <ImageView
        android:id="@+id/iconSearch"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.632"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.476"
        app:srcCompat="@drawable/icon_search_nav" />

    <ImageView
        android:id="@+id/iconUser"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.839"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.476"
        app:srcCompat="@drawable/icon_user_checked" />

    <View
        android:id="@+id/PerfilPagamentos"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/circle_container"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/containerPagamentos"
        app:layout_constraintStart_toStartOf="@+id/containerPagamentos"
        app:layout_constraintTop_toTopOf="@+id/containerPagamentos"></View>

    <View
        android:id="@+id/containerPagamentos"
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/container_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.372"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/containerNotificacoes"></View>

    <ImageView
        android:id="@+id/iconeHistoric"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="15dp"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/PerfilPagamentos"
        app:layout_constraintEnd_toEndOf="@+id/PerfilPagamentos"
        app:layout_constraintStart_toStartOf="@+id/PerfilPagamentos"
        app:layout_constraintTop_toTopOf="@+id/PerfilPagamentos"
        app:layout_constraintVertical_bias="0.333"
        app:srcCompat="@drawable/icon_card" />

    <View
        android:id="@+id/PerfilCircle"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/circle_container"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/containerPerfil"
        app:layout_constraintStart_toStartOf="@+id/containerPerfil"
        app:layout_constraintTop_toTopOf="@+id/containerPerfil"></View>

    <View
        android:id="@+id/containerPerfil"
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/container_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PhotoFrame"></View>

    <ImageView
        android:id="@+id/iconeUsuario"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:elevation="4dp"
        android:onClick="goperfil"
        app:layout_constraintBottom_toBottomOf="@+id/PerfilCircle"
        app:layout_constraintEnd_toEndOf="@+id/PerfilCircle"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintStart_toStartOf="@+id/PerfilCircle"
        app:layout_constraintTop_toTopOf="@+id/PerfilCircle"
        app:layout_constraintVertical_bias="0.518"
        app:srcCompat="@drawable/icon_user_profile" />

    <TextView
        android:id="@+id/perfil"
        android:layout_width="wrap_content"
        android:layout_height="26dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:onClick="goperfil"
        android:text="@string/perfil"
        android:textAppearance="@style/perfil"
        app:layout_constraintBottom_toBottomOf="@+id/containerPerfil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.012"
        app:layout_constraintStart_toEndOf="@+id/iconeUsuario"
        app:layout_constraintTop_toTopOf="@+id/containerPerfil"
        app:layout_constraintVertical_bias="0.409" />


    <TextView
        android:id="@+id/configura_e"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:onClick="goperfil"
        android:text="@string/configura_e"
        android:textAppearance="@style/configura_e"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/containerPerfil"
        app:layout_constraintEnd_toEndOf="@+id/containerPerfil"
        app:layout_constraintHorizontal_bias="0.222"
        app:layout_constraintStart_toEndOf="@+id/PerfilCircle"
        app:layout_constraintTop_toTopOf="@+id/containerPerfil"
        app:layout_constraintVertical_bias="0.484" />

    <View
        android:id="@+id/containerNotificacoes"
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/container_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.372"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/containerPerfil"></View>


    <View
        android:id="@+id/PerfilNotif"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/circle_container"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/containerNotificacoes"
        app:layout_constraintStart_toStartOf="@+id/containerNotificacoes"
        app:layout_constraintTop_toTopOf="@+id/containerNotificacoes"></View>

    <ImageView
        android:id="@+id/iconeNotific"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/PerfilNotif"
        app:layout_constraintEnd_toEndOf="@+id/PerfilNotif"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="@+id/PerfilNotif"
        app:layout_constraintTop_toTopOf="@+id/PerfilNotif"
        app:layout_constraintVertical_bias="0.571"
        app:srcCompat="@drawable/icon_notif_perfil" />

    <TextView
        android:id="@+id/notifica_es"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:text="@string/notifica_es"
        android:textAppearance="@style/notifica_es"
        app:layout_constraintBottom_toBottomOf="@+id/containerNotificacoes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/PerfilNotif"
        app:layout_constraintTop_toTopOf="@+id/containerNotificacoes"
        app:layout_constraintVertical_bias="0.388" />

    <TextView
        android:id="@+id/minha_centr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="10dp"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:text="@string/minha_centr"
        android:textAppearance="@style/minha_centr"
        app:layout_constraintBottom_toBottomOf="@+id/containerNotificacoes"
        app:layout_constraintEnd_toEndOf="@+id/containerNotificacoes"
        app:layout_constraintHorizontal_bias="0.181"
        app:layout_constraintStart_toEndOf="@+id/PerfilNotif"
        app:layout_constraintTop_toTopOf="@+id/containerNotificacoes"
        app:layout_constraintVertical_bias="0.705" />

    <TextView
        android:id="@+id/meus_saldos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:text="@string/meus_saldos"
        android:textAppearance="@style/meus_saldos"
        app:layout_constraintBottom_toBottomOf="@+id/containerPagamentos"
        app:layout_constraintEnd_toEndOf="@+id/containerPagamentos"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/PerfilPagamentos"
        app:layout_constraintTop_toTopOf="@+id/containerPagamentos"
        app:layout_constraintVertical_bias="0.754" />


    <View
        android:id="@+id/PerfilConfig"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/circle_container"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/containerConfig"
        app:layout_constraintStart_toStartOf="@+id/containerConfig"
        app:layout_constraintTop_toTopOf="@+id/containerConfig"></View>

    <View
        android:id="@+id/containerConfig"
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/container_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.372"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/containerPagamentos"></View>

    <ImageView
        android:id="@+id/iconeConfig"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="20dp"
        android:elevation="6dp"
        app:layout_constraintBottom_toBottomOf="@+id/PerfilConfig"
        app:layout_constraintEnd_toEndOf="@+id/PerfilConfig"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="@+id/PerfilConfig"
        app:layout_constraintTop_toTopOf="@+id/PerfilConfig"
        app:layout_constraintVertical_bias="0.333"
        app:srcCompat="@drawable/icon_config_profile" />

    <TextView
        android:id="@+id/config"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:text="Preferências"
        android:textAppearance="@style/config"
        app:layout_constraintBottom_toBottomOf="@+id/containerConfig"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/PerfilConfig"
        app:layout_constraintTop_toTopOf="@+id/containerConfig"
        app:layout_constraintVertical_bias="0.34" />

    <TextView
        android:id="@+id/configuracoes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:text="@string/configuracoes"
        android:textAppearance="@style/configuracoes"
        app:layout_constraintBottom_toBottomOf="@+id/containerConfig"
        app:layout_constraintEnd_toEndOf="@+id/containerConfig"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/PerfilConfig"
        app:layout_constraintTop_toTopOf="@+id/containerConfig"
        app:layout_constraintVertical_bias="0.673" />

    <TextView
        android:id="@+id/pagamentos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:elevation="4dp"
        android:fontFamily="@font/inter_black"
        android:gravity="center_vertical"
        android:text="@string/pagamentos"
        android:textAppearance="@style/notifica_es"
        app:layout_constraintBottom_toBottomOf="@+id/containerPagamentos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/PerfilPagamentos"
        app:layout_constraintTop_toTopOf="@+id/containerPagamentos"
        app:layout_constraintVertical_bias="0.388" />


</androidx.constraintlayout.widget.ConstraintLayout>